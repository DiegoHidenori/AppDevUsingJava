public class Main {
    public static void main(String[] args) {
        Book harryPotter = new Book("Harry Potter", 100, "JK Rowling");
        System.out.println(harryPotter.printNumPages());


    }
}
public class Book {
    public String name;
    public int pages;
    public String author;

    public Book (String name, int pages, String author) {
        this.name = name;
        this.pages = pages;
        this.author = author;
    }

    public int printNumPages(){
        return pages;
    }
}
